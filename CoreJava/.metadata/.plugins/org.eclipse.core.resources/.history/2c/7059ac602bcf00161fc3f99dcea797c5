import dao.UserDAO;

@Controller
public class LoginController {
	
	UserDAO udao;
	@RequestMapping("/validate")
	public String validate(@ModelAttribute("user")User user){
		
		udao.SelectUser(userId, password)
		return "/admin/admin";
	}
	
}
